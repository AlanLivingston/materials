   "source": [
    "from functools import lru_cache\n",
    "import profile\n",
    "\n",
    "@lru_cache\n",
    "def factorial(num):\n",
    "    if num == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return num * factorial(num-1)"
   ]
  },
